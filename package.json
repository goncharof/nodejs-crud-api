{
  "name": "nodejs-crud-api",
  "version": "0.1.0",
  "description": "task is to implement simple CRUD API using in-memory database underneath",
  "scripts": {
    "start:dev": "nodemon",
    "start:prod": "npm run build && node dist/server.js",
    "start:multi": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/multi-start.ts",
    "test": "jest --silent",
    "test:verbose": "jest",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goncharof/nodejs-crud-api.git"
  },
  "keywords": [
    "nodejs",
    "rs-school",
    "crud-api"
  ],
  "author": "goncharof",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/goncharof/nodejs-crud-api/issues"
  },
  "homepage": "https://github.com/goncharof/nodejs-crud-api#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^16.4.1",
    "uuid": "^9.0.1"
  }
}
